<ul>
<% @user.gangs.each do |gang| %>
  <ul>
  <% gang.posts.each do |post|  %>
    <li><%= post.title %></li>
    <ul>
      <li><%= post.user.username %></li>
      <li><%= post.body %></li>
      <ul>
        <% post.links.each do |link| %>
